@model BeyazMasa.Entity.Concrete.Duyurular
@{
    ViewBag.Title = "DuyuruDetay";
    Layout = "~/Views/Shared/_LayoutBelediye.cshtml";
}

<div class="header">
    <h1 class="page-header">
        Anasayfa <small>Hoşgeldiniz Deneme Belediyesi</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#">Beyaz Masa</a></li>
        <li><a href="#">Duyurular</a></li>
        <li class="active">Detay</li>
    </ol>

</div>

<div id="page-inner" class="text-center">

    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    @Model.Baslik
                </div>
                <div class="panel-body">
                    <p>@Model.Icerik</p>
                </div>
                <div class="panel-footer">
                    Duyuru Eklenme Tarihi : @Model.Tarih
                </div>
            </div>

        </div>
    </div>

</div>